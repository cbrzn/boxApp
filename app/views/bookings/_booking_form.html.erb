<% unless current_user?(@user) %>
      <% if current_user.bookings(@training) %>
        <%= link_to "Reservar", new_training_booking_path(@training), class: "btn btn-primary" %>
      <% else %>
        <%= link_to "Eliminar reserva", unbook_path, method: "delete", class: "btn btn-primary" %>
    <% end %>
<% end %>
